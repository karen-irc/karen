{
  "name": "karen",
  "description": "The self-hosted Web IRC client",
  "version": "0.0.0",
  "preferGlobal": true,
  "engines": {
    "node": "^6.0.0",
    "npm": "^3.8.6"
  },
  "bin": {
    "karen": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/karen-irc/karen.git"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node index.js",
    "clean": "gulp clean",
    "build": "gulp build",
    "build_client": "gulp build:client",
    "build_server": "gulp build:server",
    "pretest": "gulp test",
    "test": "npm-run-all --sequential test:**",
    "test:client": "node ./tools/test_launcher.js --manifest client/test_manifest.js",
    "test:lib": "node ./tools/test_launcher.js --manifest lib/test_manifest.js",
    "lint": "npm-run-all --parallel lint_**",
    "lint_js": "npm-run-all --parallel eslint tslint",
    "eslint": "eslint --ext .js,.jsx . ./**/.eslintrc.js",
    "tslint": "tslint --project ./tsconfig.json",
    "lint_css": "stylelint src/style/**/* --config ./stylelint.config.js -f verbose --color",
    "tsc": "gulp tsc",
    "gulp": "gulp"
  },
  "keywords": [
    "browser",
    "web",
    "chat",
    "client",
    "irc",
    "server"
  ],
  "maintainers": [
    {
      "name": "Yusuke Suzuki",
      "email": "utatane.tea@gmail.com",
      "web": "https://github.com/Constellation"
    },
    {
      "name": "Tetsuharu OHZEKI",
      "email": "saneyuki.s.snyk@gmail.com",
      "web": "https://github.com/saneyuki"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "cheerio": "^0.20.0",
    "commander": "^2.9.0",
    "compression": "^1.6.2",
    "cookies-js": "^1.2.2",
    "event-stream": "^3.3.3",
    "express": "^4.14.0",
    "express-session": "^1.14.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.14.1",
    "option-t": "^2.0.2",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "read": "^1.0.7",
    "request": "^2.74.0",
    "rxjs": "5.0.0-beta.10",
    "slate-irc": "karen-irc/slate-irc.git#b12d540fa231b64e4e012c9e29412a2dc448acbe",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "spdy": "^3.3.4",
    "urijs": "^1.18.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.0",
    "del": "^2.2.1",
    "eslint": "^3.2.2",
    "eslint-plugin-node": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "event-stream": "^3.3.4",
    "glob": "^7.0.5",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-postcss": "^6.1.1",
    "gulp-uglify": "^2.0.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.0",
    "npm-run-all": "^2.3.0",
    "stylelint": "^7.1.0",
    "tslint": "3.14.0-dev.1",
    "typescript": "2.1.0-dev.20160802",
    "typings": "^1.3.2",
    "webpack": "^1.13.1",
    "whatwg-fetch": "^1.0.0",
    "yargs": "^4.8.1"
  }
}
